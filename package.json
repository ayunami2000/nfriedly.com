{
  "name": "nfriedly.com",
  "version": "1.0.0",
  "description": "Personal website of Nathan Friedly, built with DocPad",
  "dependencies": {
    "docpad": "~6.79.0",
    "docpad-plugin-cleanurls": "~2.8.1",
    "docpad-plugin-eco": "~2.1.0",
    "docpad-plugin-highlightjs": "~2.4.0",
    "docpad-plugin-jshint": "~2.2.1",
    "docpad-plugin-less": "~2.4.1",
    "docpad-plugin-livereload": "~2.7.1",
    "docpad-plugin-marked": "~2.3.0",
    "docpad-plugin-partials": "~2.9.2",
    "docpad-plugin-redirector": "~2.0.3",
    "docpad-plugin-sitemap": "~2.2.0",
    "docpad-plugin-thumbnails": "~2.1.2",
    "underscore": "^1.8.3"
  },
  "main": "node_modules/docpad/bin/docpad-server",
  "scripts": {
    "start": "docpad run",
    "build": "docpad generate --env static --out docs/",
    "deploy": "npm run build; git add --all docs/; git commit -m 'Deploying on `date`'; git push"
  },
  "license": "MIT",
  "repository": "https://github.com/nfriedly/nfriedly.com",
  "devDependencies": {}
}
